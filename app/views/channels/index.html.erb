<h5>Channel List</h5>
<div class="row">
  <ul>
    <% @channels.each do |channel| %>
      <li><%= link_to channel.name, channel_path(channel.uuid) %></li>
    <% end %>
  </ul>
</div>


<h5>Join New Channel</h5>

<%= simple_form_for @channel, html: { :class => "form-horizontal" } do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.submit "Join" %>
<% end %>
